{
  "name": "sandbox",
  "version": "0.0.1",
  "type": "module",
  "imports": {
    "#*": "./src/*"
  },
  "scripts": {
    "dev": "vite dev",
    "deploy": "bun run build && bun x wrangler@latest --config='./dist/server/wrangler.json' deploy --keep-vars",
    "build": "NODE_ENV='production' vite build --config='vite.config.ts'",
    "preview": "vite preview",
    "build:types": "wrangler --config='wrangler.json' types",
    "check": "biome check . --fix --unsafe",
    "check:types": "bun tsgo --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@cloudflare/sandbox": "^0.7.0",
    "@solid-primitives/active-element": "^2.1.3",
    "@solid-primitives/bounds": "^0.1.3",
    "@solid-primitives/clipboard": "^1.6.2",
    "@solid-primitives/connectivity": "^0.4.3",
    "@solid-primitives/context": "^0.3.2",
    "@solid-primitives/event-dispatcher": "^0.1.1",
    "@solid-primitives/event-listener": "^2.4.3",
    "@solid-primitives/keyboard": "^1.3.3",
    "@solid-primitives/platform": "^0.2.1",
    "@solid-primitives/resize-observer": "^2.1.3",
    "@solid-primitives/scheduled": "^1.5.2",
    "@solid-primitives/scroll": "^2.1.3",
    "@solid-primitives/storage": "^4.3.3",
    "@tanstack/solid-router": "^1.157.16",
    "@tanstack/solid-start": "^1.157.16",
    "@tanstack/start-static-server-functions": "^1.157.16",
    "@xterm/xterm": "^5.6.0-beta.140",
    "ghostty-web": "^0.3.0",
    "solid-js": "^1.9.11",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@cloudflare/vite-plugin": "^1.22.0",
    "@shikijs/transformers": "^3.21.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/devtools-vite": "^0.4.1",
    "@tanstack/solid-router-devtools": "^1.157.16",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/bun": "^1.3.7",
    "@types/node": "^25.1.0",
    "@typescript/native-preview": "^7.0.0-dev.20260128.1",
    "crossws": "^0.4.4",
    "eruda": "^3.4.3",
    "node-pty": "beta",
    "nypm": "^0.6.4",
    "shiki": "^3.21.0",
    "sonda": "^0.10.1",
    "tailwindcss": "^4.1.18",
    "typed-query-selector": "^2.12.0",
    "typescript": "^5.9.3",
    "unplugin-info": "^1.2.4",
    "vite": "^8.0.0-beta.11",
    "vite-plugin-devtools-json": "^1.0.0",
    "vite-plugin-inspect": "^11.3.3",
    "vite-plugin-solid": "^2.11.10",
    "vite-tsconfig-paths": "^6.0.5",
    "wrangler": "^4.61.0"
  },
  "private": true,
  "license": "MIT",
  "repository": "o-az/sandbox"
