{
  "name": "foundry-sandbox",
  "version": "0.0.0",
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "bun x wrangler@latest --config='wrangler.json' deploy --keep-vars",
    "build": "bun x wrangler@latest --config='wrangler.json' deploy --keep-vars --dry-run",
    "build:types": "bun x wrangler@latest --config='wrangler.json' types --include-runtime false",
    "check": "biome check . --fix --unsafe",
    "check:types": "bun tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@cloudflare/sandbox": "^0.4.15"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.3",
    "@cloudflare/workers-types": "^4.20251014.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/bun": "^1.3.1",
    "@types/node": "^24.10.0",
    "@xterm/addon-clipboard": "^0.1.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-ligatures": "^0.9.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "typed-query-selector": "^2.12.0",
    "typescript": "^5.9.3",
    "wrangler": "^4.45.4"
  },
  "private": true,
  "license": "MIT",
  "repository": "o-az/foundry-sandbox"
}
